<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clipChildren="false"
    android:clipToPadding="false"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/icon_room"
        android:layout_width="@dimen/sp_4xl"
        android:layout_height="@dimen/sp_4xl"
        android:layout_margin="@dimen/sp_md"
        android:padding="@dimen/sp_sm"
        android:background="@drawable/shape_squircle_inactive"
        android:elevation="@dimen/sp_0"
        android:outlineSpotShadowColor="?attr/colorPrimary"
        android:src="@drawable/bx_landscape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:targetApi="p"
        tools:ignore="ContentDescription" />

</androidx.constraintlayout.widget.ConstraintLayout>